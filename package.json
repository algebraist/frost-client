{
  "name": "@po.et/frost-client",
  "version": "0.1.8",
  "description": "Po.et - Frost client",
  "main": "dist/Frost.js",
  "types": "dist/Frost.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "tslint -p ./tsconfig.json",
    "lint:fix": "tslint -p ./tsconfig.json --fix",
    "lint:check": "tslint-config-prettier-check ./tslint.json",
    "test:unit": "ts-node --files tests/unit/index.ts",
    "coverage": "nyc --report-dir ./.coverage npm run test:unit",
    "precommit": "lint-staged",
    "compile": "tsc",
    "prepublish": "npm run compile",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "*.ts": [
      "tslint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poetapp/frost-client.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/poetapp/frost-client/issues"
  },
  "homepage": "https://github.com/poetapp/frost-client#readme",
  "dependencies": {
    "@po.et/poet-js": "2.0.0",
    "es6-promise": "4.2.4",
    "isomorphic-fetch": "2.2.1",
    "serialize-javascript": "1.5.0"
  },
  "devDependencies": {
    "@po.et/tslint-rules": "1.4.0",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/serialize-javascript": "1.3.2",
    "@types/sinon": "5.0.1",
    "husky": "0.14.3",
    "lint-staged": "7.2.0",
    "nyc": "github:poetapp/nyc#fbc2ed1",
    "prettier": "1.14.2",
    "riteway": "2.0.4",
    "semantic-release": "15.9.8",
    "sinon": "6.0.1",
    "ts-node": "7.0.0",
    "tslint-config-prettier": "1.14.0",
    "tslint-plugin-prettier": "1.3.0",
    "typescript": "3.0.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
